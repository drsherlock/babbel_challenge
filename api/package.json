{
  "name": "api",
  "version": "0.1.0",
  "private": true,
  "description": "Babbel Coding Challenge API",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node --experimental-specifier-resolution=node src/index.js",
    "start:dev": "nodemon --experimental-specifier-resolution=node src/index.js",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:dev": "node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "format": "prettier -w ./src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "ioredis": "^4.28.0",
    "morgan": "^1.10.0",
    "prettier": "^2.6.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.1.1",
    "jest": "^26.5.3",
    "nodemon": "^2.0.4",
    "supertest": "^5.0.0"
  }
}
